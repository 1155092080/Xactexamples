Cell["The basic relations between these two charts", "Text",ExpressionUUID->"4c88f28c-9769-49bf-8723-85e73ee69c56"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"cart2sph", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"x", "[", "]"}], "^", "2"}], "+", 
       RowBox[{
        RowBox[{"y", "[", "]"}], "^", "2"}], "+", 
       RowBox[{
        RowBox[{"z", "[", "]"}], "^", "2"}]}], "]"}], ",", 
     RowBox[{"ArcCos", "[", 
      RowBox[{
       RowBox[{"z", "[", "]"}], "/", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"x", "[", "]"}], "^", "2"}], "+", 
         RowBox[{
          RowBox[{"y", "[", "]"}], "^", "2"}], "+", 
         RowBox[{
          RowBox[{"z", "[", "]"}], "^", "2"}]}], "]"}]}], "]"}], ",", 
     RowBox[{"ArcTan", "[", 
      RowBox[{
       RowBox[{"x", "[", "]"}], ",", 
       RowBox[{"y", "[", "]"}]}], "]"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"sph2cart", "=", 
   RowBox[{
    RowBox[{"r", "[", "]"}], 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"\[Theta]", "[", "]"}], "]"}], 
       RowBox[{"Cos", "[", 
        RowBox[{"\[Phi]", "[", "]"}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"\[Theta]", "[", "]"}], "]"}], 
       RowBox[{"Sin", "[", 
        RowBox[{"\[Phi]", "[", "]"}], "]"}]}], ",", 
      RowBox[{"Cos", "[", 
       RowBox[{"\[Theta]", "[", "]"}], "]"}]}], "}"}]}]}], ";"}]}], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"7d7e3571-d469-43ed-bece-78dd6eaa2924"],

